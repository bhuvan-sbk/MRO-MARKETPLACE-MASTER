{
    "version": 2,
    "builds": [
      {
        "src": "backend/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "frontend/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/index.html"
      }
    ]
  }